mixin  navigation_content
	ul
		li: a(href="{{site.baseurl}}/index.html") HOME
				li: a(href="#") ABOUT  
				li: a(href="#") CONTACT
				li: a(href="#") 
				li: a(href="#") 

.header_navigation_mobile
		+navigation_content

header.inside_wrapper
	.header_wrapper
		nav.header_navigation
			+navigation_content
				
		img.mobile_navigation_button(onclick="toggleMenu()")(src="{{site.baseurl}}/assets/img/menu.svg")
			


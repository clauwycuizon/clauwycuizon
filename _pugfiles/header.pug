mixin  navigation_content
	ul
		li: a(href="{{site.baseurl}}/index.html") HOME
		li: a(href="#") ABOUT  
		li: a(href="#") CONTACT
		

.header_navigation_mobile
	#mobile_Nav.mobile_Nav_content
		+navigation_content

header.inside_wrapper
	.header_wrapper
		nav.header_navigation
			+navigation_content
		
		button.mobile_navigation_button(onclick="toggleMenu()")
			

